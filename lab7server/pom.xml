    <build>
        <plugins>

            <!-- Плагин Maven Compiler для использования аннотационного процессора -->
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.9.0</version>
                <executions>
                    <execution>
                        <id>querydsl</id>
                        <goals>
                            <goal>compile</goal>
                        </goals>
                        <configuration>
                            <proc>only</proc>
                            <!-- Добавление пути к аннотационному процессору Querydsl -->
                            <annotationProcessorPaths>
                                <path>
                                    <groupId>com.querydsl</groupId>
                                    <artifactId>querydsl-apt</artifactId>
                                    <version>${querydsl.version}</version>
                                </path>
                                <!-- Добавление пути к артефакту Jakarta Persistence API -->
                                <path>
                                    <!-- https://mvnrepository.com/artifact/jakarta.persistence/jakarta.persistence-api -->
                                    <groupId>jakarta.persistence</groupId>
                                    <artifactId>jakarta.persistence-api</artifactId>
                                    <version>3.1.0</version>

                                </path>
                            </annotationProcessorPaths>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>build-helper-maven-plugin</artifactId>
                <version>3.5.0</version>
                <executions>
                    <execution>
                        <id>clean</id>
                        <goals>
                            <goal>clean</goal>
                        </goals>
                        <configuration>
                            <resources>
                                <resource>
                                    <!-- Путь к сгенерированным файлам Querydsl -->
                                    <directory>${project.build.directory}/generated-sources/annotations/org/example/entity/</directory>
                                </resource>
                            </resources>
                        </configuration>
                    </execution>
                </executions>
            </plugin>

